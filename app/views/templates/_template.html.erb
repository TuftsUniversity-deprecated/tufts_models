<tr>
  <td> <%= link_to "Edit", hydra_editor.edit_record_path(template[:id]), class: "btn btn-mini" %> </td>
  <td> <%= template[:id] %> </td>
  <td> <%= template[:template_name_tesim].first unless template[:template_name_tesim].nil? %> </td>
  <td> <%= Date.parse(template[:edited_at_dtsi]).to_s %> </td>
  <td> <%= template[:edit_access_person_ssim].first unless template[:edit_access_person_ssim].nil? %></td>
  <td> <%= link_to "Delete", record_path(template[:id]), method: :delete, class: "btn btn-danger btn-mini",  data: {confirm: "WARNING: this will permanently delete template #{template[:id]}! Are you sure you want to proceed?"} %> </td>
</tr>



